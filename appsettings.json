{
  "Logging": {
      "LogLevel": {
          "Default": "Warning"
      }
  },
  "endpoints": {
    "order-service": "order-service.internal"
  },
  "AllowedHosts": "*",
  "management": {
      "endpoints": {
          "path": "/cloudfoundryapplication",
          "cloudfoundry": {
              "validateCertificates": false
          }
      },
      "metrics": {
          "exporter": {
              "cloudfoundry": {
                  "validateCertificates": false
              }
          }
      }
  }
}